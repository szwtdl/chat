{
    "name": "szwtdl/chat",
    "description": "chat live im laravel-chat websocket",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "ext-swoole": ">=4.4"
    },
    "require-dev": {
        "swoole/ide-helper": "^5.0",
        "friendsofphp/php-cs-fixer": "^3.10",
        "mockery/mockery": "^1.4.4",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^9.5",
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "Szwtdl\\Chat\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "szwtdl",
            "email": "szpengjian@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Szwtdl\\Chat\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
